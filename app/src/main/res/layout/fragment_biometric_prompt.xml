<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/morado"
        tools:context=".FingerPrint.BiometricPromptFragment">

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnHuellaAutentication"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="27dp"
            android:layout_marginTop="24dp"
            android:layout_marginRight="27dp"
            android:background="@drawable/btn_redondo_trasparente_blanco_generico"
            android:orientation="vertical"
            android:text="@string/msj_huella_autenticacion_auth"
            android:textAllCaps="false"
            android:textColor="@color/blanco"
            android:textSize="14sp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />



    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>